{
  "name": "node-misrcon",
  "version": "0.3.12",
  "author": "Chris Sprance",
  "main": "dist/index.js",
  "description": "package that contains functions to send rcon commands to miscreated game servers",
  "contributors": [
    {
      "name": "Chris Sprance",
      "email": "chrissprance@gmail.com"
    }
  ],
  "scripts": {
    "watch:source": "rimraf dist/ && babel src/ --watch --out-dir dist/",
    "watch:tests": "rimraf test-dist/ && babel test/ --watch --out-dir test-dist/",
    "dev": "gulp watch",
    "prebublish": "npm run build",
    "lint": "eslint src test",
    "flow": "flow status",
    "build:source": "rimraf dist/ && babel src/ --out-dir dist/",
    "build:tests": "rimraf test-dist/ && babel test/ --out-dir test-dist/",
    "clean": "rimraf dist/ && rimraf test-dist/",
    "build": "npm run build:source && npm run build:tests",
    "test": "npm run build && node node_modules/mocha/bin/mocha --reporter spec test-dist/*",
    "example": "node example.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/csprance/node-misrcon.git"
  },
  "keywords": [
    "miscreated",
    "rcon",
    "misrcon"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "6.24.1",
    "chai": "3.5.0",
    "eslint": "3.6.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "flow-bin": "^0.52.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-eslint": "3.0.1",
    "gulp-mocha": "3.0.1",
    "gulp-rimraf": "0.2.0",
    "mocha": "3.1.0",
    "rimraf": "2.5.4",
    "run-sequence": "1.2.2"
  },
  "engine": "node >= 0.10.x",
  "homepage": "https://github.com/csprance/node-misrcon",
