{
  "name": "node-misrcon",
  "version": "0.7.3",
  "author": "Chris Sprance",
  "main": "./dist/index.js",
  "types": "./index.d.ts",
  "description": "package that contains functions to send rcon commands to miscreated game servers",
  "contributors": [
    {
      "name": "Chris Sprance",
      "email": "chrissprance@gmail.com"
    }
  ],
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "scripts": {
    "prebublish": "npm run jest && npm run build",
    "repl": "ts-node ./repl.js",
    "test": "jest",
    "clean": "rimraf -rf ./dist",
    "build": "npm run clean && tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/csprance/node-misrcon.git"
  },
  "keywords": [
    "miscreated",
    "rcon",
    "misrcon"
  ],
  "devDependencies": {
    "@types/jest": "22.2.2",
    "@types/md5": "2.1.32",
    "@types/node": "9.6.0",
    "@types/xml2js": "0.4.2",
    "jest": "22.4.3",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.2",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "engine": "node >= 0.10.x",
  "homepage": "https://github.com/csprance/node-misrcon",
  "bugs": {
    "url": "https://github.com/csprance/node-misrcon/issues"
  },
  "private": false,
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "md5": "^2.2.1",
    "xml2js": "^0.4.17"
  }
}
